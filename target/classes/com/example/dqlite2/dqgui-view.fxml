<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<HBox alignment="CENTER" spacing="5.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.dqlite2.DQGUIController">
    <VBox alignment="CENTER_LEFT" spacing="10.0" xmlns:fx="http://javafx.com/fxml">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Label fx:id="heroName" text="heroName"/>
        <Label fx:id="heroLevel" text="heroLevel"/>
        <Label fx:id="heroHP" text="heroHP"/>
        <Label fx:id="heroMP" text="heroMP"/>
        <Label text="Current Enemy"/>
        <Label fx:id="enemyName" text="enemyName"/>
        <Label fx:id="enemyLevel" text="enemyLevel"/>
    </VBox>
    <VBox alignment="CENTER_RIGHT" spacing="10.0" xmlns:fx="http://javafx.com/fxml">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>

        <TextField/>

        <HBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml">
            <Label text="Enter a Command"/>
            <TextField fx:id="commandLine"/>
            <Button text="Submit" onAction="#onSubmitButtonClick"/>
        </HBox>
    </VBox>
</HBox>
